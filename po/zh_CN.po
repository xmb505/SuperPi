#: superpi.c:58
msgid "Usage: %s [digits]\n"
msgstr "用法：%s [位数]\n"

#: superpi.c:65
msgid "Error: Invalid number of digits. Please specify between 1 and %d.\n"
msgstr "错误：无效的位数。请指定1到%d之间的数字。\n"

#: superpi.c:71
msgid "SuperPi - Calculating π to %d digits...\n"
msgstr "SuperPi - 正在计算π到%d位...\n"

#: superpi.c:79
msgid "Calculation completed in %.2f seconds\n"
msgstr "计算完成，耗时%.2f秒\n"

#: superpi.c:85
msgid "Error: Memory allocation failed or calculation error.\n"
msgstr "错误：内存分配失败或计算错误。\n"

#: superpi.c:93
msgid "Results saved to file.\n"
msgstr "结果已保存到文件。\n"

#: superpi.c:101
msgid "SuperPi - A high-performance pi calculation tool\n\n"
msgstr "SuperPi - 高性能圆周率计算工具\n\n"

#: superpi.c:103
msgid "  digits    Number of digits to calculate (default: %d)\n"
msgstr "  位数    要计算的圆周率位数（默认：%d）\n"

#: superpi.c:105
msgid "\nOptions:\n"
msgstr "\n选项：\n"

#: superpi.c:106
msgid "  -h, --help     Show this help message\n"
msgstr "  -h, --help     显示此帮助信息\n"

#: superpi.c:107
msgid "  -v, --version  Show version information\n"
msgstr "  -v, --version  显示版本信息\n"

#: superpi.c:109
msgid "\nExamples:\n"
msgstr "\n示例：\n"

#: superpi.c:110
msgid "  %s 1000000    Calculate 1 million digits\n"
msgstr "  %s 1000000    计算100万位\n"

#: superpi.c:111
msgid "  %s 10000000   Calculate 10 million digits\n"
msgstr "  %s 10000000   计算1000万位\n"

#: superpi.c:116
msgid "Copyright (c) 2025 新毛宝贝 (xmb505)\n"
msgstr "版权所有 (c) 2025 新毛宝贝 (xmb505)\n"

#: superpi.c:117
msgid "Blog: blog.xmb505.top\n"
msgstr "博客：blog.xmb505.top\n"

#: superpi.c:189
msgid "Error: Cannot create file %s\n"
msgstr "错误：无法创建文件 %s\n"

#: superpi.c:197
msgid "Calculated by SuperPi\n"
msgstr "由SuperPi计算\n"

#: superpi.c:198
msgid "Digits: %d\n"
msgstr "位数：%d\n"

#: superpi.c:199
msgid "Date: %s"
msgstr "日期：%s"

#: superpi.c:202
msgid "Results saved to: %s\n"
msgstr "结果已保存到：%s\n"